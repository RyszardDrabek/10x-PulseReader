{
  "name": "10x-astro-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:ssr": "astro dev --mode production",
    "build": "astro build",
    "preview": "astro preview",
    "preview:cloudflare": "npm run build && npx wrangler dev",
    "dev:cloudflare": "npm run build && npx wrangler dev --local",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit:coverage && npm run test:e2e",
    "lighthouse": "lhci autorun",
    "performance": "npm run lighthouse",
    "load-test": "k6 run load-test.js",
    "security": "npm run security:npm && npm run security:container",
    "security:npm": "npm audit && snyk test",
    "security:container": "trivy image --exit-code 1 --no-progress --format table",
    "coverage:upload": "codecov",
    "storybook": "historia",
    "storybook:build": "historia build"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.11",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "isomorphic-dompurify": "^2.32.0",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/coverage-v8": "^2.1.9",
    "codecov": "^3.8.2",
    "dotenv": "^17.2.3",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "happy-dom": "^20.0.10",
    "historia": "^0.1.0",
    "husky": "9.1.7",
    "k6": "^0.0.0",
    "lighthouse-ci": "^1.13.1",
    "lint-staged": "15.5.0",
    "msw": "^2.12.2",
    "playwright": "^1.56.1",
    "prettier-plugin-astro": "0.14.1",
    "typescript-eslint": "8.28.0",
    "vitest": "^2.1.9",
    "wrangler": "^4.49.0"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
